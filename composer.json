{
    "name": "goodwong/laravel-wechat",
    "description": "Laravel微信基础模块，提供微信用户资料模型及微信账号的配置信息",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "william",
            "email": "goodwong@foxmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "overtrue/laravel-wechat": "~3.0",
        "prettus/l5-repository": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Goodwong\\Wechat\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Goodwong\\Wechat\\WechatServiceProvider"
            ]
        }
    }
}
